generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int     @id @default(autoincrement())
  first_name  String?
  last_name   String?
  email       String  @unique
  profile_pic String?
  username    String?
  password    String?
}
